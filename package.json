{
  "name": "webext-storage-cache",
  "version": "2.1.1",
  "description": "WebExtensions module: Map-like promised cache storage with expiration. Chrome and Firefox.",
  "keywords": [
    "await",
    "background page",
    "browser",
    "cache",
    "chrome",
    "content script",
    "expirating",
    "extension",
    "firefox",
    "map",
    "options page",
    "promises",
    "self-cleaning",
    "temporary",
    "web-ext",
    "webext"
  ],
  "repository": "fregante/webext-storage-cache",
  "license": "MIT",
  "author": "Connor Love",
  "contributors": [
    "Federico Brigante <opensource@bfred.it> (bfred.it)"
  ],
  "files": [
    "index.js",
    "index.d.ts",
    "module/index.js"
  ],
  "module": "index.js",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "tsc",
    "test": "run-s build test:*",
    "test:xo": "xo",
    "test:tsd": "tsd; true",
    "watch": "tsc --watch"
  },
  "xo": {
    "envs": [
      "browser",
      "webextensions"
    ],
    "extensions": [
      "ts"
    ],
    "overrides": [
      {
        "files": "**/*.ts",
        "extends": "xo-typescript",
        "parserOptions": {
          "project": "./tsconfig.xo.json"
        }
      }
    ]
  },
  "dependencies": {
    "webext-detect-page": "^1.0.1"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^0.6.0",
    "@types/chrome": "0.0.91",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "eslint-config-xo-typescript": "^0.23.0",
    "npm-run-all": "^4.1.5",
    "tsd": "^0.11.0",
    "typescript": "^3.7.3",
    "xo": "*"
  },
  "type": "module"
}
